{
  "name": "ti-module-linea-pro",
  "version": "0.1.0",
  "description": "A Titanium module wrapping the Linea Pro hardware SDK for iOS",
  "scripts": {
    "postinstall": "node ./appc-npm"
  },
  "author": "Edwin Bosveld",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/inwy/linea-pro.git"
  },
  "homepage": "https://github.com/inwy/linea-pro",
  "bugs": {
    "url": "https://github.com/inwy/linea-pro/issues"
  },
  "files": [
    "appc-npm",
    "ios/dist/nl.inwy.LineaPro-iphone-0.1.0.zip"
  ],
  "appc-npm": {
    "unzip": [
      "ios/dist/nl.inwy.LineaPro-iphone-0.1.0.zip"
    ],
    "tiapp": [
      {
        "name": "nl.inwy.LineaPro",
        "version": "0.1.0",
        "platform": "iphone"
      }
    ],
    "target": {
      "titanium": "/",
      "alloy": "/"
    }
  },
  "keywords": [
    "alloy",
    "appc-npm",
    "appcelerator",
    "ti-module",
    "titanium"
  ]
}
